{
  "collectCoverageFrom": [
    "src/**/*.{js,jsx}"
  ],
  "coverageThreshold": {
    "global": {
      "statements": 100,
      "branches": 100,
      "functions": 100,
      "lines": 100
    }
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "testPathIgnorePatterns": [
    "/node_modules/"
  ],
  "testRegex": "__tests__/.*\\.test\\.js$"
}
